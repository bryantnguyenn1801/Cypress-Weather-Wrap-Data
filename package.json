{
  "devDependencies": {
    "cypress": "12.3.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-mochawesome-reporter": "3.2.3",
    "marge": "1.0.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "4.2.2",
    "mochawesome-report-generator": "6.2.0"
  },
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:test": "cypress run",
    "cypress:test mocha reporter": "cypress run --reporter mochawesome --reporter-options reportDir=\"cypress/results\",overwrite=false,html=false,json=true",
    "cypress:merge-report": "mochawesome-merge \"mochawesome-report/*.json\" > mochawesome.json",
    "cypress:build-report": "marge mochawesome.json"
  },
  "dependencies": {
    "cypress-xpath": "2.0.1",
    "moment": "2.29.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
